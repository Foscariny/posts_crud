<div>
    <h1>New users</h1>

    <%= form_for @user do |f|%>
        <%= f.label :username %>
        <%= f.text_field :username %><br>
        <%= f.label :email %>
        <%= f.text_field :email %><br>
        <%= f.label :password %>
        <%= f.password_field :password %><br>
        <%= f.label :password_confirmation %>
        <%= f.password_field :password_confirmation %><br>
        <%= submit_tag 'Create account' %>
    <% end %>
</div>

